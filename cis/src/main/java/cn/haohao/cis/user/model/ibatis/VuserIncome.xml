			<if test="newUpdAttObj.performance != null">t.PERFORMANCE = #{newUpdAttObj.performance},</if>	
		</set>
		<include refid="VuserIncome.whereClause" />
	</update>
	
	<delete id="VuserIncome.delete">
		<![CDATA[
		delete from V_USER_INCOME t
		]]>
		<include refid="VuserIncome.whereClause" />
